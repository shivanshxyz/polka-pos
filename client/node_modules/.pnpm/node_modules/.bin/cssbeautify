#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/shivanshxyz/repos/polka-pos2/client/node_modules/.pnpm/cssbeautify@0.3.1/node_modules/cssbeautify/bin/node_modules:/Users/shivanshxyz/repos/polka-pos2/client/node_modules/.pnpm/cssbeautify@0.3.1/node_modules/cssbeautify/node_modules:/Users/shivanshxyz/repos/polka-pos2/client/node_modules/.pnpm/cssbeautify@0.3.1/node_modules:/Users/shivanshxyz/repos/polka-pos2/client/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/shivanshxyz/repos/polka-pos2/client/node_modules/.pnpm/cssbeautify@0.3.1/node_modules/cssbeautify/bin/node_modules:/Users/shivanshxyz/repos/polka-pos2/client/node_modules/.pnpm/cssbeautify@0.3.1/node_modules/cssbeautify/node_modules:/Users/shivanshxyz/repos/polka-pos2/client/node_modules/.pnpm/cssbeautify@0.3.1/node_modules:/Users/shivanshxyz/repos/polka-pos2/client/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../cssbeautify/bin/cssbeautify" "$@"
else
  exec node  "$basedir/../cssbeautify/bin/cssbeautify" "$@"
fi
