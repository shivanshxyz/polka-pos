#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/shivanshxyz/repos/polka-pos2/client/node_modules/.pnpm/@polkadot-api+cli@0.11.11_jiti@2.4.2_postcss@8.5.3/node_modules/@polkadot-api/cli/dist/node_modules:/Users/shivanshxyz/repos/polka-pos2/client/node_modules/.pnpm/@polkadot-api+cli@0.11.11_jiti@2.4.2_postcss@8.5.3/node_modules/@polkadot-api/cli/node_modules:/Users/shivanshxyz/repos/polka-pos2/client/node_modules/.pnpm/@polkadot-api+cli@0.11.11_jiti@2.4.2_postcss@8.5.3/node_modules/@polkadot-api/node_modules:/Users/shivanshxyz/repos/polka-pos2/client/node_modules/.pnpm/@polkadot-api+cli@0.11.11_jiti@2.4.2_postcss@8.5.3/node_modules:/Users/shivanshxyz/repos/polka-pos2/client/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/shivanshxyz/repos/polka-pos2/client/node_modules/.pnpm/@polkadot-api+cli@0.11.11_jiti@2.4.2_postcss@8.5.3/node_modules/@polkadot-api/cli/dist/node_modules:/Users/shivanshxyz/repos/polka-pos2/client/node_modules/.pnpm/@polkadot-api+cli@0.11.11_jiti@2.4.2_postcss@8.5.3/node_modules/@polkadot-api/cli/node_modules:/Users/shivanshxyz/repos/polka-pos2/client/node_modules/.pnpm/@polkadot-api+cli@0.11.11_jiti@2.4.2_postcss@8.5.3/node_modules/@polkadot-api/node_modules:/Users/shivanshxyz/repos/polka-pos2/client/node_modules/.pnpm/@polkadot-api+cli@0.11.11_jiti@2.4.2_postcss@8.5.3/node_modules:/Users/shivanshxyz/repos/polka-pos2/client/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@polkadot-api/cli/dist/main.js" "$@"
else
  exec node  "$basedir/../@polkadot-api/cli/dist/main.js" "$@"
fi
