#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/shivanshxyz/repos/polka-pos2/client/node_modules/.pnpm/vite-node@3.0.0-beta.2_@types+node@20.17.30_lightningcss@1.29.2/node_modules/vite-node/node_modules:/Users/shivanshxyz/repos/polka-pos2/client/node_modules/.pnpm/vite-node@3.0.0-beta.2_@types+node@20.17.30_lightningcss@1.29.2/node_modules:/Users/shivanshxyz/repos/polka-pos2/client/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/shivanshxyz/repos/polka-pos2/client/node_modules/.pnpm/vite-node@3.0.0-beta.2_@types+node@20.17.30_lightningcss@1.29.2/node_modules/vite-node/node_modules:/Users/shivanshxyz/repos/polka-pos2/client/node_modules/.pnpm/vite-node@3.0.0-beta.2_@types+node@20.17.30_lightningcss@1.29.2/node_modules:/Users/shivanshxyz/repos/polka-pos2/client/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../vite-node@3.0.0-beta.2_@types+node@20.17.30_lightningcss@1.29.2/node_modules/vite-node/vite-node.mjs" "$@"
else
  exec node  "$basedir/../../../../../../vite-node@3.0.0-beta.2_@types+node@20.17.30_lightningcss@1.29.2/node_modules/vite-node/vite-node.mjs" "$@"
fi
