#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/shivanshxyz/repos/polka-pos2/client/node_modules/.pnpm/polkadot-api@1.10.0_jiti@2.4.2_postcss@8.5.3_rxjs@7.8.2/node_modules/polkadot-api/bin/node_modules:/Users/shivanshxyz/repos/polka-pos2/client/node_modules/.pnpm/polkadot-api@1.10.0_jiti@2.4.2_postcss@8.5.3_rxjs@7.8.2/node_modules/polkadot-api/node_modules:/Users/shivanshxyz/repos/polka-pos2/client/node_modules/.pnpm/polkadot-api@1.10.0_jiti@2.4.2_postcss@8.5.3_rxjs@7.8.2/node_modules:/Users/shivanshxyz/repos/polka-pos2/client/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/shivanshxyz/repos/polka-pos2/client/node_modules/.pnpm/polkadot-api@1.10.0_jiti@2.4.2_postcss@8.5.3_rxjs@7.8.2/node_modules/polkadot-api/bin/node_modules:/Users/shivanshxyz/repos/polka-pos2/client/node_modules/.pnpm/polkadot-api@1.10.0_jiti@2.4.2_postcss@8.5.3_rxjs@7.8.2/node_modules/polkadot-api/node_modules:/Users/shivanshxyz/repos/polka-pos2/client/node_modules/.pnpm/polkadot-api@1.10.0_jiti@2.4.2_postcss@8.5.3_rxjs@7.8.2/node_modules:/Users/shivanshxyz/repos/polka-pos2/client/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/cli.mjs" "$@"
else
  exec node  "$basedir/../../bin/cli.mjs" "$@"
fi
