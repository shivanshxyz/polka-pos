#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/shivanshxyz/repos/polka-pos2/client/node_modules/.pnpm/@react-router+dev@7.5.2_@react-router+serve@7.5.2_react-router@7.5.2_react-dom@19.1.0_r_b7941856de693b578c20b0cc6c57d83e/node_modules/@react-router/dev/node_modules:/Users/shivanshxyz/repos/polka-pos2/client/node_modules/.pnpm/@react-router+dev@7.5.2_@react-router+serve@7.5.2_react-router@7.5.2_react-dom@19.1.0_r_b7941856de693b578c20b0cc6c57d83e/node_modules/@react-router/node_modules:/Users/shivanshxyz/repos/polka-pos2/client/node_modules/.pnpm/@react-router+dev@7.5.2_@react-router+serve@7.5.2_react-router@7.5.2_react-dom@19.1.0_r_b7941856de693b578c20b0cc6c57d83e/node_modules:/Users/shivanshxyz/repos/polka-pos2/client/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/shivanshxyz/repos/polka-pos2/client/node_modules/.pnpm/@react-router+dev@7.5.2_@react-router+serve@7.5.2_react-router@7.5.2_react-dom@19.1.0_r_b7941856de693b578c20b0cc6c57d83e/node_modules/@react-router/dev/node_modules:/Users/shivanshxyz/repos/polka-pos2/client/node_modules/.pnpm/@react-router+dev@7.5.2_@react-router+serve@7.5.2_react-router@7.5.2_react-dom@19.1.0_r_b7941856de693b578c20b0cc6c57d83e/node_modules/@react-router/node_modules:/Users/shivanshxyz/repos/polka-pos2/client/node_modules/.pnpm/@react-router+dev@7.5.2_@react-router+serve@7.5.2_react-router@7.5.2_react-dom@19.1.0_r_b7941856de693b578c20b0cc6c57d83e/node_modules:/Users/shivanshxyz/repos/polka-pos2/client/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@react-router/dev/bin.js" "$@"
else
  exec node  "$basedir/../@react-router/dev/bin.js" "$@"
fi
